<Window x:Class="MiSS_Symulacja.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MiSS_Symulacja"
        mc:Ignorable="d"
        Title="MiSS Symulacja" Height="500" Width="700" ResizeMode="CanMinimize">
    <Grid x:Name="MainPanel">
        <Canvas x:Name="SimulationPanel" Margin="0"/>
        <Button x:Name="LineSim" Content="Whadlo na lince" Width="130" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,140,10" Click="LineSimButton_Click" />
        <Button x:Name="SpringSim" Content="Wahadlo na sprężynie" Width="130" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="140,0,0,10" Click="SpringSimButton_Click" />
    </Grid>
</Window>
